{
    "name": "szwtdl/laravel-asiabill",
    "description": "Asiabill payment sdk, asiabill安信易",
    "type": "library",
    "license": "Apache-2.0",
    "require": {
        "ext-json": "*",
        "asiabill/asiabill_php_sdk": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "jetbrains/phpstorm-attributes": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.11",
        "phpstan/phpstan": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Asiabill\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "szwtdl",
            "email": "szpengjian@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Asiabill\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
